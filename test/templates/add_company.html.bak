<div id="the-form">
	<table>	
		<tr><td colspan="2"><b>Required Information * :</b></td></tr>
		<tr><td class="form-elem"><label class="zform">Company Name*</label>:</td><td><input type="text" id="co_name"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Associated Email Address</label>:</td><td><input type="text" id="co_email"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Login Name*</label>:</td><td><input type="text" id="co_login"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Password*</label>:</td><td><input type="password" id="co_pass1"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Confirm Password*</label>:</td><td><input type="password" id="co_pass2"/></td></tr>
		<tr><td colspan="2"><b>Address:</b></td></tr>
		<tr><td class="form-elem"><label class="zform">Street 1*</label>:</td><td><input type="text" id="co_addr1"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Street 2</label>:</td><td><input type="text" id="co_addr2"/></td></tr>
		<tr><td class="form-elem"><label class="zform">City</label>:</td><td><input type="text" id="co_city"/></td></tr>
		<tr><td class="form-elem"><label class="zform">State/Country/Province*</label>:</td><td><input type="text" id="co_state"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Country*</label>:</td><td><select id="co_country">
			<option value="uk">United Kingdom</option>
			<option value="sg">Singapore</option>
			<option value="us">United States</option>
			<option value="fr">France</option>
			<option value="es">Spain</option>
			</select></td></tr>
		<tr><td class="form-elem"><label class="zform">Zip/Postal Code*</label>:</td><td><input type="text" id="co_postal"/></td></tr>
		<tr><td colspan="2"><b>Online Information:</b></td></tr>
		<tr><td class="form-elem"><label class="zform">Company Website*</label>:</td><td><input type="text" id="co_website"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Company Facebook Page</label>:</td><td><input type="text" id="co_facebook"/></td></tr>
		<tr><td class="form-elem"><label class="zform">Captcha</label>:</td><td>ABCDEF</td></tr>
		<tr><td class="form-elem"><label class="zform">Type the text above*</label>:</td><td><input type="text" id="co_captcha"/></td></tr>
	</table>
</div>
<div class="form-elem"><button onclick="form_submit()">Register</button></div>
